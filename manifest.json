{
  "manifest_version": 2,

  "name": "Maple7Sha",
  "description": "This extension extract a list of member names when logged as administrator.",
  "version": "1.0",

  "permissions": [
    "tabs",
    "https://trunk.tufts.edu/",
    "https://*/",
    "http://*/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
